pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'master', url: 'https://github.com/ateeqhonyal321/pjen'
            }
        }

        stage('Build') {
            steps {
                echo "Building.."
            }
        }

        stage('Test') {
            steps {
                echo "Testing.."
            }
        }
    }
}
